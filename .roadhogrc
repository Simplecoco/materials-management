{
  "entry": "src/index.js",
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr",
        "transform-runtime",
        ["import", { "libraryName": "antd", "style": "css" }]
      ]
    },
    "production": {
      "extraBabelPlugins": [
        "transform-runtime",
        ["import", { "libraryName": "antd", "style": "css"  }]
      ]
    }
  },
  "proxy": {
    "/api": {
      "target": "http://asset.starstudio.org/v1/supply/",
      "changeOrigin": true,
      "pathRewrite": { "^/api": "" }
    },
    "/v1/supply": {
      "target": "http://asset.starstudio.org/v1/supply/",
      "changeOrigin": true,
      "pathRewrite": { "^/v1/supply": "" }
    }
  },
  "less": true
}
